RULES=contest
#ITUMULT
SHORT_SERIAL
EDITOR=vi
CALL=DF7CB
TIME_OFFSET=0
TIME_MASTER
NETKEYER
NETKEYERPORT=6789
NETKEYERHOST=127.0.0.1
#---speed (6 ... 60 wpm)
CWSPEED=30
#---weight (-5 ... 5 ms)
WEIGHT=1
#---cq delay (in 0,5 s)
CQDELAY=10
#---txdelay (ms)
TXDELAY=2
#---sidetone (200...800, 0 = mute)
CWTONE=800
